{
    "version": 3,
    "build": {
      "commands": [
        "npx prisma db push",
        "npx prisma generate"
      ]
    },
    "use": "@vercel/node",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",  
        "methods": ["GET", "POST", "PUT", "DELETE","OPTIONS"]
      }
    ]
  }